{
    "name":"__xlog__",
     "version":"1.0",
     "manifest_version":2,
     "description":"",
     "background":{
         "scripts":["js/background.js"],
         "persistent":true
     },
    "browser_action": {
        "default_popup": "popup.html" 
    },
    "content_scripts":[
    {    
        "matches":["<all_urls>"],
        "js":["js/content.js"]  
    }],
    "permissions":[
        "storage", "tabs"
    ]
}

